#!/bin/bash

TOP_DIR=$(cd $(dirname $0) && pwd)/..

source ${TOP_DIR}/config $*

[ -z "$HOST" ] && HOST=`docker inspect --format '{{ .NetworkSettings.IPAddress }}' $CONTAINER_NAME`
[ -z "$PORT" ] && PORT=$LOCAL_WEBSSH_PORT

URL="https://localhost:$PORT/?ssh=ssh://ubuntu:ubuntu@$HOST:$SSH_PORT/"


# FIXME: chromium-browser doesn't support Chinese input for gateone now
# Only firefox works.
(firefox $URL 2>&1>/dev/null &) 2>&1>/dev/null
